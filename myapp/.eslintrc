{
    "extends":["react-app","prettier"],
    "plugins": ["react", "prettier"],
    "rules": {
        "prettier/prettier":[
            "warn",
            {
                "semi": true,
                "trailingComma": "none",
                "endOfLine":"auto",
                "tabWidth": 4,
                "useTabs": false,
                "singleQuote": true,
                "printWidth": 80,
                "jsxSingleQuote": false
            }
        ]
    }
}