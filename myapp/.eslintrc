{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "extends":["react-app","prettier","eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "plugins": ["react", "prettier","@typescript-eslint"],
    "rules": {
        "prettier/prettier":[
            "warn",
            {
                "semi": true,
                "trailingComma": "all",
                "endOfLine":"auto",
                "tabWidth": 4,
                "useTabs": false,
                "singleQuote": false,
                "printWidth": 80,
                "jsxSingleQuote": false
            }
        ]
    }
}